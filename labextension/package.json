{
  "private": true,
  "name": "jupyterlab_json_labextension",
  "version": "0.1.0",
  "description": "A Jupyter Lab extension for rendering JSON",
  "author": "Grant Nestor",
  "main": "lib/plugin.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build:src": "tsc --project src && node scripts/copyfiles.js",
    "build:extension": "node scripts/build_extension.js",
    "build": "npm run build:src && npm run build:extension",
    "watch": "watch \"npm run build\" src --wait 10 --ignoreDotFiles",
    "prepublish": "npm run build",
    "extension:install": "jupyter labextension install --symlink --py --sys-prefix jupyterlab_json",
    "extension:uninstall": "jupyter labextension uninstall --py --sys-prefix jupyterlab_json",
    "extension:enable": "jupyter labextension enable --py --sys-prefix jupyterlab_json",
    "extension:disable": "jupyter labextension disable --py --sys-prefix jupyterlab_json"
  },
  "dependencies": {
    "jupyterlab": ">=0.8.0",
    "phosphor": ">=0.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-highlighter": "^0.3.3",
    "react-json-tree": "^0.10.0"
  },
  "devDependencies": {
    "@jupyterlab/extension-builder": ">=0.8",
    "@jupyterlab/services": ">=0.34.2",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-highlighter": "0.0.2",
    "@types/react-json-tree": "^0.6.1",
    "fs-extra": "^0.30.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.1",
    "tslint-react": "^2.0.0",
    "typescript": "^2.0.10",
    "watch": "^1.0.1"
  }
}
